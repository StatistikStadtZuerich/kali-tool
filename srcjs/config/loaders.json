[
  {
    "test": "\\.css$",
    "use": [
      "style-loader",
      {
        "loader": "css-loader",
        "options": {
          "import": true,
          "modules": true
        }
      }
    ]
  },
  {
    "test": "\\.s[ac]ss$",
    "use": [
      "style-loader",
      "css-loader",
      "sass-loader"
    ]
  }
]
